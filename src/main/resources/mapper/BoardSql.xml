<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTDMapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="boot.mvc.board.BoardMapperInter">
	<insert id="insertStyle" parameterType="BoardDto">
	insert into board (board_id,user_num,board_content,board_image,board_like_count,board_comment_count,board_writeday)
	</insert>
	
	<select id="getAllList" resultType="BoardDto">
	select * from board order by board_id
	</select>
</mapper>